<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
 
 <bean id = "myteam" class = "com.Soccer.team.Team">
 <property name="teamName" value ="Real Madrid"></property>
 <property name="teamCouch" value= "Zinadin zidan"></property>
 <property name="teamJerseyColor" value = "White"></property>
 <property name="teamStedium" value = "RFK"></property>
 <property name="teamOwner" value = "Priz"></property>
 
 <constructor-arg ref = "meal"></constructor-arg>

 </bean>
 
 <bean id = "meal" class = "com.Soccer.team.Players_Meal"></bean>
 
 
 
 
 
 
 
 </beans>